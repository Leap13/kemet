{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"link": true,
			"palette": [
                {
                    "slug": "foreground",
                    "color": "#023047",
                    "name": "Foreground"
                  },
                  {
                    "slug": "background",
                    "color": "#ffffff",
                    "name": "Background"
                  },
                  {
                    "slug": "primary",
                    "color": "#fab522",
                    "name": "Primary"
                  },
                  {
                    "slug": "secondary",
                    "color": "#eaeaea",
                    "name": "Secondary"
                  },
                  {
                    "slug": "tertiary",
                    "color": "#244c60",
                    "name": "Tertiary"
                  }
			]
		},
		"layout": {
            "contentSize": "min( calc(100vw - 2rem), 80rem)",
            "wideSize": "min( calc(100vw - 2rem), 90rem)",
            "outerPadding": "0.5rem"
		},
		"spacing": {
			"blockGap": true,
			"customPadding": true,
			"customMargin": true,
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"customFontSize": true,
			"customLineHeight": true,
			"fontFamilies": [
                {
                  "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
                  "name": "System Font",
                  "slug": "system-font"
                },
                {
                  "fontFamily": "Poppins,sans-serif",
                  "name": "Poppins",
                  "slug": "poppins",
                  "google": "family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900"
                },
                {
                  "fontFamily": "Lato,sans-serif",
                  "name": "Lato",
                  "slug": "lato",
                  "google": "family=Lato:ital,wght@0,100;0,300;0,400;0,700;0,900;1,100;1,300;1,400;1,700;1,900"
                }
              ],
              "fontSizes": [
                {
                  "slug": "small",
                  "size": "max(calc(12px + 0.4vw),15px)",
                  "name": "Small"
                },
                {
                  "slug": "medium",
                  "size": "max(calc(12px + 0.98vw),18px)",
                  "name": "Medium"
                },
                {
                  "slug": "large",
                  "size": "max(calc(12px + 1.46vw),19px)",
                  "name": "Large"
                },
                {
                  "slug": "huge",
                  "size": "max(calc(12px + 2.03vw),20px)",
                  "name": "Huge"
                },
                {
                  "slug": "colossal",
                  "size": "calc(12px + 4.45vw)",
                  "name": "Colossal"
                }
              ]
		}
	},
	"styles": {
		"blocks": {
			"core/buttons": {
				"spacing": {
					"blockGap": "calc( 1.5 * var( --wp--custom--spacing--baseline ) )"
				}
			},
			"core/button": {
				"border": {
					"color": "currentColor",
					"radius": "0px",
					"style": "var(--wp--custom--shaded-style--border--style)",
					"width": "var(--wp--custom--shaded-style--border--width)"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"padding": {
						"bottom": "0.833em",
						"left": "1.33em",
						"right": "1.33em",
						"top": "0.833em"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"core/calendar": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "calc( 2 * var( --wp--custom--spacing--baseline ) )",
						"top": "calc( 2 * var( --wp--custom--spacing--baseline ) )"
					}
				}
			},
			"core/cover": {
				"color": {
					"text": "#ffffff"
				},
				"spacing": {
					"padding": "var( --wp--custom--gutter )"
				}
			},
			"core/file": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"core/group": {
				"spacing": {
					"margin": {
						"bottom": "calc( 2 * var( --wp--custom--spacing--baseline ) )",
						"top": "calc( 2 * var( --wp--custom--spacing--baseline ) )"
					}
				}
			},
			"core/paragraph": {
				"spacing": {
					"blockGap": "1em"
				},
				"typography": {
					"lineHeight": "calc( 1em + 10px )"
				}
			},
			"core/preformatted": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"bottom": "1em",
						"top": "1em"
					}
				}
			},
			"core/pullquote": {
				"spacing": {
					"padding": {
						"bottom": "0px",
						"left": "0px",
						"right": "0px",
						"top": "0px"
					}
				}
			},
			"core/separator": {
				"spacing": {
					"margin": {
						"bottom": "calc( 2 * var( --wp--custom--spacing--baseline ) )",
						"top": "calc( 2 * var( --wp--custom--spacing--baseline ) )"
					}
				}
			},
			"core/site-tagline": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/site-title": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)"
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--body)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)",
			"link": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"spacing": {
					"margin": {
						"bottom": "calc( 1.5 * var( --wp--custom--spacing--baseline ) )",
						"top": "calc( 2 * var( --wp--custom--spacing--baseline ) )"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"h2": {
				"spacing": {
					"margin": {
						"bottom": "calc( 1.5 * var( --wp--custom--spacing--baseline ) )",
						"top": "calc( 2 * var( --wp--custom--spacing--baseline ) )"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"h3": {
				"spacing": {
					"margin": {
						"bottom": "calc( 1.5 * var( --wp--custom--spacing--baseline ) )",
						"top": "calc( 2 * var( --wp--custom--spacing--baseline ) )"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"h4": {
				"spacing": {
					"margin": {
						"bottom": "calc( 1.5 * var( --wp--custom--spacing--baseline ) )",
						"top": "calc( 2 * var( --wp--custom--spacing--baseline ) )"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"h5": {
				"spacing": {
					"margin": {
						"bottom": "calc( 1.5 * var( --wp--custom--spacing--baseline ) )",
						"top": "calc( 2 * var( --wp--custom--spacing--baseline ) )"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-5)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"h6": {
				"spacing": {
					"margin": {
						"bottom": "calc( 1.5 * var( --wp--custom--spacing--baseline ) )",
						"top": "calc( 2 * var( --wp--custom--spacing--baseline ) )"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-6)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--uppercase)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)",
					"textTransform": "uppercase"
				}
			},
			"link": {
				"color": {
					"text": "inherit"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--dm-sans)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--body)",
			"lineHeight": "var(--wp--custom--typography--line-height--body)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
    "customTemplates": [
        {
          "name": "post-sidebar-right",
          "title": "Two columns, Right sidebar",
          "postTypes": ["post"]
        },
        {
          "name": "post-sidebar-left",
          "title": "Two columns, left sidebar",
          "postTypes": ["post"]
        }
      ]
}